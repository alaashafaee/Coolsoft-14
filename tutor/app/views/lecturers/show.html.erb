<div class = "well" style = "width:170px; height:170px; background-color:#999; float:left;"></div>
<div class = "tabs" style = "float:left; margin-left:10px;">
	<ul class = "tab-links">
		<li class = "active"><a href = "#tab1">Info</a></li>
	</ul>
	<div class = "tab-content">
		<div id ="tab1" class = "tab active">
			<p> <b>Name:</b> <%= @lecturer.name %> </p>
			<p> <b>Email:</b> <%= @lecturer.email %> </p>
			<p> <b>Degree:</b> <%= @lecturer.degree %> </p>
			<p> <b>Department:</b> <%= @lecturer.department %> </p>
			<p> <b>University:</b> <%= @lecturer.university %> </p>
			<p> <b>Joined on:</b> <%= @lecturer.created_at.to_date %> </p>
		</div>
	</div>
</div>
