<% if objects.present? %>
	<div class="panel panel-default">
		<div class="panel-title list-group-item list-group-item-info"
				data-parent="#accordion" 
				href="<%= objects.first.class.name.pluralize.to_s %>">
			<div align="center">
				<a class="panel-title">
					<%= objects.first.class.name.pluralize.to_s %>
				</a>
			</div>
		</div>
		<div id="<%= objects.first.class.name.pluralize.to_s %>">
			<ul class="list-group">
				<% objects.each do |object| %>
					<li class="list-group-item" data-toggle="tooltip"
						data-placement="top" title="<%= object.name%>">
						<% unless object.name.nil? %>
							<%= link_to object.name, object ,class: 'color' %>
						<% else %>
							<%= link_to object.title, object ,class: 'color' %>
						<% end %>
					</li>
				<% end %>
			</ul>
		</div>
	</div>
<% end %>